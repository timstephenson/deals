#nav li:first-child { background: none; }

#nav li a {
    color: #fffefe;
    text-decoration: none;
    background: none;
}

#nav li a:hover,
#nav li.active a {
    color: #c41230;
}


.ribbon {
    background: url(<%= asset_path 'wcax/assets/images/jumponit/ribbon.png' %>) no-repeat;
    width: 18px;
    height: 65px;
    position: absolute;
    
}
.ribbon-topleft {
    background-position: left top;
    left: -18px;
    top: 0;
}
.ribbon-topright {
    background-position: right top;
    width: 17px;
    right: -17px;
    top: 0;
}

.ribbon-bottomleft {
    background-position: left bottom;
    left: -18px;
    bottom: 0;
}
.ribbon-bottomright {
    background-position: right bottom;
    width: 17px;
    right: -17px;
    bottom: 0;
}

