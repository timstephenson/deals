    font-style: normal;
}
pre {
    background: #f4f4f4;
    font: 13px "Courier 10 Pitch",Courier,monospace;
    line-height: 1.5;
    margin-bottom: 1.5em;
    padding: 0.75em 1.5em;
}
code, kbd {
    font: 13px Monaco,Consolas,"Andale Mono","DejaVu Sans Mono",monospace;
}
abbr, acronym {
    border-bottom: 1px dotted #666;
    cursor: help;
}
ins {
    background: #fff9c0;
    text-decoration: none;
}
sup,
sub {
    font-size: 10px;
    height: 0;
    line-height: 1;
    position: relative;
    vertical-align: baseline;
}
sup {
    bottom: 1ex;
}
sub {
    top: .5ex;
}


/* Links */
