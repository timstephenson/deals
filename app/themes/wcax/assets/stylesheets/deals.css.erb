    width:17px;
    height:37px;
}

#nav li {
    background: url(<%= asset_path 'wcax/assets/images/jumponit/sep_nav.png' %>) left 2px no-repeat;
    float: left;
    font-size: 15px;
    list-style: none;
    text-transform: uppercase;
    padding-left: 16px;
    margin-left: 15px;
    line-height: normal;
