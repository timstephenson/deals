.add_new_publisher{
    background: #cdeb8e; /* Old browsers */
    /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(top,  #cdeb8e 0%, #a5c956 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#cdeb8e), color-stop(100%,#a5c956)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top,  #cdeb8e 0%,#a5c956 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top,  #cdeb8e 0%,#a5c956 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(top,  #cdeb8e 0%,#a5c956 100%); /* IE10+ */
    background: linear-gradient(top,  #cdeb8e 0%,#a5c956 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#cdeb8e', endColorstr='#a5c956',GradientType=0 ); /* IE6-8 */
    border: 1px solid #A7CB45;
    cursor: pointer;
    -webkit-border-radius: 6px;
    -moz-border-radius: 6px;
    
    border-radius: 6px;
    -webkit-box-shadow: inset 0px 1px 1px #E0EAC9, inset 0px -1px 1px #7FA111, 0 0 3px #ABABAB;
    -moz-box-shadow: inset 0px 1px 1px #E0EAC9, inset 0px -1px 1px #7FA111, 0 0 3px #ABABAB;
    box-shadow: inset 0px 1px 1px #E0EAC9, inset 0px -1px 1px #7FA111, 0 0 3px #ABABAB;
    color: #315400;
    font-size: 1.2em;
    line-break: 1.2;
    padding: 8px;
    text-shadow: 1px 1px 0 #D1FF50;
    -webkit-transition: all .5s ease-in;
    -moz-transition: all .5s ease-in;
    -o-transition: all .5s ease-in;
    -ms-transition: all .5s ease-in;
    transition: all .5s ease-in;
