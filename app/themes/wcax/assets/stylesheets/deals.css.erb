/* Type */
body { font: 0.74em Arial, Helvetica, sans-serif; line-height: 1.5em; background: #fff; color: #454545; }

a {
  color: #999999;
  background: inherit;
  text-decoration: none;
}

a:hover {
  color: #C3593C;
  background: inherit;
}

a#what-is-cvv {
